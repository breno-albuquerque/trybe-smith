{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
        "airbnb-base",
        "airbnb-typescript/base",
        "plugin:editorconfig/noconflict"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-console": "off",
      "camelcase": "warn",
      "consistent-return": "off",
      "object-curly-newline": "off"
    }
}
